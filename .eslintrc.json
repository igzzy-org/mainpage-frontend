{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "rules": {
        "import/extensions": 0
    }
}